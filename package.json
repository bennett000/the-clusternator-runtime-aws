{
  "name": "the-clusternator-runtime-aws",
  "version": "0.0.1",
  "description": "The Clusternator's CI runtime for AWS backed clusternations",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover _mocha --R ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rangle/the-clusternator-runtime-aws.git"
  },
  "keywords": [
    "clusternator",
    "ci",
    "cd",
    "deployment",
    "testing"
  ],
  "author": "Rangle.io",
  "license": "MIT",
  "bin": {
    "clusternator-runtime-aws": "./bin/clusternator.sh"
  },
  "bugs": {
    "url": "https://github.com/rangle/the-clusternator-runtime-aws/issues"
  },
  "homepage": "https://github.com/rangle/the-clusternator-runtime-aws#readme",
  "os": [
    "!win32"
  ],
  "dependencies": {
    "aws-sdk": "^2.2.41"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "generators": true
    },
    "rules": {
      "comma-dangle": 0,
      "max-len": [
        1,
        80,
        4
      ],
      "no-console": 2,
      "no-func-assign": 2,
      "no-multi-spaces": 0,
      "no-undef": 2,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "no-var": 2,
      "new-cap": 0,
      "one-var": [
        2,
        "never"
      ],
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "valid-typeof": 2
    }
  }
}
